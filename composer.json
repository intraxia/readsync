{
  "name": "maadhattah/readsync",
  "type": "wordpress-plugin",
  "license": "GPL-2.0+",
  "description": "ReadSync backs up your reading list from Pocket, using PressForward's import process",
  "homepage": "https://github.com/mAAdhaTTah/readsync",
  "authors": [
    {
      "name": "James DiGioia",
      "email": "jamesorodig@gmail.com",
      "homepage": "http://www.jamesdigioia.com/"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "webdevstudios/cmb2": "2.0.5",
    "wpupdatephp/wp-update-php": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.3.*",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "ReadSync\\": "app/"
    },
    "files": ["lib/webdevstudios/cmb2/init.php"]
  },
  "config": {
    "vendor-dir": "lib"
  }
}
